    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.66",
    "@mui/material": "^5.3.1",
    "@mui/system": "^5.3.0",
    "canvas-confetti": "^1.5.1",
    "dayjs": "^1.10.7",
    "events": "^3.3.0",
    "gatsby": "^4.6.0",
    "gatsby-plugin-manifest": "^4.10.0",
    "gatsby-plugin-material-ui": "^4.0.2",
    "gatsby-plugin-offline": "^5.6.0",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-typescript": "^4.5.2",
    "gatsby-source-local-git-modern": "^2.0.1",
    "gatsby-source-package": "^1.0.0",
    "hammerjs": "^2.0.8",
    "is-in-browser": "^2.0.0",
