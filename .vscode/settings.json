{
  "cSpell.words": [
    "bindgen",
    "blks",
    "blockchanged",
    "blockschanged",
    "Borked",
    "Coord",
    "deps",
    "easytimer",
    "fira",
    "fluentui",
    "fontawesome",
    "fontsource",
    "fortawesome",
    "gameended",
    "gamestarted",
    "gamestatechanged",
    "googlefonts",
    "lightgreen",
    "localstorage",
    "notistack",
    "noto",
    "Offseted",
    "opentype",
    "periotris",
    "periotrisjs",
    "precache",
    "pressup",
    "reduxjs",
    "roboto",
    "Rong",
    "Segoe",
    "semibold",
    "structs",
    "tetrimino",
    "tetriminos",
    "topo",
    "toposort",
    "typegen",
    "Unmount",
    "viewmodel",
    "workerloader"
  ],
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.detectIndentation": true,
  "importSorter.importStringConfiguration.hasSemicolon": false,
  "importSorter.importStringConfiguration.quoteMark": "double",
  "importSorter.importStringConfiguration.tabSize": 2,
  "importSorter.sortConfiguration.customOrderingRules.rules": [
    {
      "type": "importMember",
      "regex": "^$",
      "orderLevel": 5,
      "disableSort": true
    },
    {
      "regex": "^[^.@]",
      "orderLevel": 10
    },
    {
      "regex": "^[@]",
      "orderLevel": 15
    },
    {
      "regex": "^[.]",
      "orderLevel": 30
    }
  ],
  "importSorter.sortConfiguration.removeUnusedImports": true,
  "editor.tabSize": 2,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "files.associations": {
    "*.schema": "json"
  }
}
